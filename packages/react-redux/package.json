{
  "name": "@kaf/react-redux",
  "version": "2.6.6",
  "description": "The practice of kaf on react + redux",
  "repository": "https://github.com/Graceji/kaf/tree/main/packages/react-redux",
  "author": "Graceji",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "types": "types/",
  "files": [
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts}",
    "build:types": "rimraf ./types && tsc --build ./src",
    "build:es6": "cross-env NODE_TAG=es6 babel ./src --out-dir ./dist/es6  --extensions \".ts\"",
    "build:es5": "cross-env NODE_TAG=es5 babel ./src --out-dir ./dist/es5  --extensions \".ts\"",
    "build": "yarn build:types && rimraf ./dist && yarn build:es6 && yarn build:es5"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18",
    "react-redux": "^7 || ^8"
  },
  "dependencies": {
    "@kaf/core": "^2.6.6"
  },
  "gitHead": "ad2ffab42fef29ba9f2bc0c9a1a81cd08d794794"
}
